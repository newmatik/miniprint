{
    "name": "Miniprint",
    "build": {
        "dockerfile": "Dockerfile",
        "context": ".."
    },
    "settings": { 
        "terminal.integrated.shell.linux": "/bin/bash"
    },
    "extensions": [
        "ms-python.python"
    ],
    "forwardPorts": [5500],
    "postCreateCommand": "/workspaces/<repository-name>/.devcontainer/create-env.sh",
    "remoteUser": "root"
}
